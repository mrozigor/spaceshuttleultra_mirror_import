<UserControl x:Class="SSUWorkbench.SSME"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:SSUWorkbench"
    mc:Ignorable="d" 
    d:DesignHeight="600" d:DesignWidth="800">
    <Grid>
        <CheckBox x:Name="ckbLogData" Content="Log Controller Data" HorizontalAlignment="Left" Height="20" Margin="180,25,0,0" VerticalAlignment="Top" Width="210" ToolTip="Log SSME controller data at the Engine Interface Unit" IsChecked="{Binding mission.LogSSMEData, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</UserControl>
